-- addition
imu sum : uint = 2 + 2;

-- subtraction
imu difference : uint = 2 - 2;

-- multiplication
imu product : uint = 2 * 2;

-- division
imu quotient : uint = 2 / 1;

-- remainder
imu remainder : uint = 2 % 1;

-- less than
imu lt : uint = 2 < 1;

-- greater than
imu gt : uint = 1 > 1;

-- less than or equal
imu le : uint = 1 <= 1;

-- greater than or equal
imu ge : uint = 1 >= 1;

-- equals
imu eq : uint = 1 == 1;

-- not equals
imu ne : uint = 1 != 1;