-- binray number
imu bin : uint = 0b111_111_11001_0000;

-- octal number
imu oct : uint = 0o0761;

-- integer number
imu dec : uint = 1000;

-- hexadecimal number
imu hex : uint = 0xA0A0;

-- parse to binary number
imu bina : uint = b#30; -- 0b1111

-- parse to octal number
imu octa : uint = o#75; -- 0o113

-- parse to integer number
imu deci : uint = d#25.0; -- 25

-- parse to hexadecimal number
imu hexa : uint = h#76; -- 0xA