fun main() {
  -- less than
  imu lt : uint = 2 < 1;

  -- greater than
  imu gt : uint = 1 > 1;

  -- less than or equal
  imu le : uint = 1 <= 1;

  -- greater than or equal
  imu ge : uint = 1 >= 1;

  -- equals
  imu eq : uint = 1 == 1;

  -- not equals
  imu ne : uint = 1 != 1;

  -- or
  imu or : bool = 1 < 2 || 1 == 1;

  -- and
  imu and : bool = 1 < 2 && 1 == 1;
}