fun main := () {
  val button: Button = Button {
    id = 0,
    name = "button-name",
  };

  val button: Button = new::Button("button-name");
}

struct Button {
  id: int,
  name: str,
}

set Button {
  fun new: Fun(str) -> Button = (name) {
    Button {
      id = 0,
      name = name,
    }
  }
}
